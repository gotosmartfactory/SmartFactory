{
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "IoTHubMessages",
      "direction": "in",
      "eventHubName": "smartfacto-dev",
      "connection": "eventhub_cnstr",
      "cardinality": "one",
      "consumerGroup": "func"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "cosmosOutput",
      "databaseName": "Building",
      "collectionName": "RoomDatas",
      "createIfNotExists": "true",
      "connectionStringSetting": "cosmos_cnstr",
      "partitionKey": "/twinId"
    },
    {
      "type": "eventHub",
      "direction": "out",
      "name": "adxHub",
      "connection": "adxhub_cnstr",
      "eventHubName": "adx-hub"
    }
  ],
  "scriptFile": "../dist/HubToDT/index.js"
}
